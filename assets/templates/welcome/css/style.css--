div.languages {
	margin-top: 2px;
	margin-right: 3px;
	float: right;
}

div.logo_partners {
	margin-top: 3px; 
    float: right;
}

div.logo_partners ul {
    list-style-type: none;
}

div.logo_partners ul li {
    float: left;
    padding: 5px;
}

div.logo_partners ul li a img {
	border: 1px solid #d1d1d1;
    padding: 3px;
	height: 45px;
}

h1.title_publ {
    font-size: 2em;
    line-height: 1.5em;
}

ul.timeline li.fix span.date, ul.timeline li.fix span.time {
	padding-right: 15px;
}

div.breaking-news {
    background: none repeat scroll 0 0 #ffffff;
    border: 1px solid #e02222;
    border-radius: 1px;
    float: left;
    margin: 0 auto 25px;
    overflow: hidden;
    position: relative;
    width: 100%;
}

div.breaking-news span {
    background: none repeat scroll 0 0 #e02222;
    color: #ffffff;
    display: block;
    float: left;
    padding: 6px 10px;
}

div.breaking-news ul {
    float: left;
    margin: 0;
}

div.breaking-news ul a {
    color: #000;
    display: block;
    padding: 6px;
    white-space: nowrap;
    z-index: 10000;
}
.page-logo h1 {
